{
    "agents": {
        "defaults": {
            "model": {
                "primary": "zhipu/glm-4.7"
            },
            "contextTokens": 128000,
            "sandbox": {
                "mode": "non-main"
            }
        }
    },
    "models": {
        "providers": {
            "zhipu": {
                "baseUrl": "https://open.bigmodel.cn/api/coding/paas/v4",
                "apiKey": "ZHIPU_API_KEY_PLACEHOLDER",
                "api": "openai-completions",
                "models": [
                    {
                        "id": "glm-4.7",
                        "name": "GLM 4.7",
                        "contextWindow": 128000,
                        "maxTokens": 8192
                    }
                ]
            }
        }
    },
    "gateway": {
        "mode": "local",
        "bind": "lan",
        "port": 18789,
        "controlUi": {
            "allowInsecureAuth": true
        }
    },
    "channels": {
        "feishu": {
            "enabled": true,
            "appId": "FEISHU_APP_ID_PLACEHOLDER",
            "appSecret": "FEISHU_APP_SECRET_PLACEHOLDER",
            "domain": "FEISHU_DOMAIN_PLACEHOLDER",
            "requireMention": true,
            "dmPolicy": "pairing",
            "groupPolicy": "allowlist"
        }
    }
}